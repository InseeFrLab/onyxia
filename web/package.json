{
    "homepage": "https://onyxia.sh",
    "name": "onyxia-web",
    "version": "3.4.1",
    "license": "MIT",
    "scripts": {
        "_comment": "Yes... We're still using CRA. We'll migrate to Vite as soon as Keycloakify gets support for it",
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "build-keycloak-theme": "yarn build && keycloakify",
        "storybook": "start-storybook -p 6006",
        "postinstall": "cp -rf patches/fs node_modules/ && patch-package && generate-env-getter && copy-keycloak-resources-to-public",
        "prestart": "generate-env-getter",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different",
        "emails-domain-accept-list-helper": "ts-node --skip-project src/bin/emails-domain-accept-list-helper.ts"
    },
    "keycloakify": {
        "themeName": "onyxia"
    },
    "dependencies": {
        "onyxia-ui": "^1.3.0",
        "keycloakify": "^8.2.2",
        "tss-react": "^4.9.3",
        "i18nifty": "^3.0.1",
        "tsafe": "^1.6.5",
        "redux-clean-architecture": "^3.7.4",
        "screen-scaler": "^1.2.5",
        "evt": "^2.5.3",
        "cra-envs": "^1.2.8",
        "oidc-spa": "^2.0.2",
        "powerhooks": "^1.0.3",
        "run-exclusive": "^2.2.19",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.8",
        "@mui/material": "^5.14.8",
        "@reduxjs/toolkit": "^1.9.1",
        "memoizee": "^0.4.14",
        "zod": "^3.19.0",
        "yaml": "^2.0.1",
        "flexsearch": "0.7.21",
        "async-mutex": "^0.4.0",
        "@ungap/structured-clone": "^1.2.0",
        "minio": "^7.1.1",
        "axios": "^0.26.0",
        "buffer": "^6.0.3",
        "color": "^4.2.3",
        "compare-versions": "^5.0.1",
        "crypto-browserify": "^3.12.0",
        "file-saver": "^2.0.2",
        "filereader-stream": "^2.0.0",
        "https-browserify": "^1.0.0",
        "minimal-polyfills": "^2.2.3",
        "moment": "^2.29.1",
        "mustache": "^4.2.0",
        "path": "^0.12.7",
        "path-browserify": "^1.0.1",
        "process": "^0.11.10",
        "react-async-hook": "^3.6.1",
        "react-code-blocks": "^0.0.9-0",
        "react-scripts": "5.0.1",
        "shell-quote": "^1.7.2",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "type-route": "^0.6.0",
        "url-join-ts": "^1.0.5"
    },
    "devDependencies": {
        "eslint-plugin-tss-unused-classes": "^1.0.2",
        "@emotion/css": "^11.11.2",
        "@storybook/addon-actions": "^6.5.14",
        "@storybook/addon-essentials": "^6.5.14",
        "@storybook/addon-events": "^6.2.9",
        "@storybook/addon-links": "^6.5.14",
        "@storybook/builder-webpack5": "^6.5.14",
        "@storybook/manager-webpack5": "^6.5.14",
        "@storybook/node-logger": "^6.5.14",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/react": "^6.5.14",
        "@types/color": "^3.0.3",
        "@types/file-saver": "^2.0.2",
        "@types/memoizee": "^0.4.4",
        "@types/minio": "^7.0.13",
        "@types/mustache": "^4.2.2",
        "@types/node": "^14.0.13",
        "@types/react": "^18.0.19",
        "@types/react-dom": "^18.0.6",
        "@types/react-router-dom": "^5.1.5",
        "@types/shell-quote": "^1.7.0",
        "@types/flexsearch": "^0.7.3",
        "@types/ungap__structured-clone": "0.3.0",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "circular-dependency-plugin": "^5.2.2",
        "eslint-config-prettier": "^8.5.0",
        "events": "^3.2.0",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "react-app-rewired": "^2.2.1",
        "patch-package": "^8.0.0",
        "ts-node": "^10.2.1",
        "typescript": "^4.8.3"
    },
    "eslintConfig": {
        "plugins": [
            "tss-unused-classes"
        ],
        "extends": [
            "react-app",
            "prettier"
        ],
        "rules": {
            "@typescript-eslint/no-redeclare": "off",
            "import/no-anonymous-default-export": "off",
            "react-hooks/rules-of-hooks": "warn",
            "react-hooks/exhaustive-deps": "off",
            "no-labels": "off",
            "prefer-const": "off",
            "no-sequences": "off",
            "tss-unused-classes/unused-classes": "warn",
            "no-lone-blocks": "off"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
